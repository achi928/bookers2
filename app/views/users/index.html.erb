<h1>ユーザーの一覧画面</h1>

<%= render 'books/book', book: @book, user: @user %>

<h2>Users</h2>

<p>image</p>
<p>name</p>
<% @users.each do |user| %>
  <div>
  <%= image_tag user.get_profile_image(100,100) %>
    <p><%= user.name %><p>
    <p><%= link_to "Show" ,user_path(user.id) %></p>
  </div>
<% end %>
